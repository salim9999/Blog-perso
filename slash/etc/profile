builtin ulimit -c 0
[ -d "/home/mobaxterm/" ] || $MKDIR -p /home/mobaxterm
[ -d "/home/$USERNAME/" ] || $LN -sf /home/mobaxterm "/home/$USERNAME" 2>/dev/null
[ -n "$MOBASTARTUPDIR" ] && cd "${MOBASTARTUPDIR}" 2>/dev/null || cd "/home/mobaxterm" 2>/dev/null

if [ "$LANG" != "fr_FR.UTF-8" ]; then
  CodeEAigu="é"
  CodeEGrave="è"
  CodeAGrave="à"
else
  CodeEAigu="Ã©"
  CodeEGrave="Ã¨"
  CodeAGrave="Ã "
